generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = "mysql://root:root@localhost:3306/simio_teste"
}

model Simio {
   id       Int     @id          @default(autoincrement())
   dna      String  @unique
   isSimian Boolen  @db.TinyInt(1)
   created_at DateTime
   @@map("simios")
}
