generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model Simio {
   id       Int     @id          @default(autoincrement())
   dna      String  @unique
   count_mutant_dna   Int     @default(0)
   count_human_dna    Int     @default(0)
   @@map("simios")
}
